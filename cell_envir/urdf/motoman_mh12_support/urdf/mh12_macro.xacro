<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:macro name="motoman_mh12" params="prefix">
        <xacro:include filename="motoman_resources/urdf/common_materials.xacro"/>
        <xacro:include filename="inertia_description/inertia_table.xacro"/>

        <!-- link list -->
        <link name="${prefix}base_link">
            <visual>
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/visual/mh12_base_link.stl"/>
                </geometry>
                <xacro:material_yaskawa_blue/>
            </visual>
            <collision>
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/collision/mh12_base_link.stl"/>
                </geometry>
            </collision>
            <inertial>
                <xacro:cylinder_inertia m="5" r="0.7" h="0.3"/>
            </inertial>
        </link>

        <link name="${prefix}link_1_s">
            <visual> 
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/visual/mh12_link_1_s.stl"/>
                </geometry>
                <xacro:material_yaskawa_blue/>
            </visual>
            <collision>
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/collision/mh12_link_1_s.stl"/>
                </geometry>
            </collision>
            <inertial>
                <xacro:cylinder_inertia m="5" r="0.5" h="0.6"/>
            </inertial>
        </link>
        <link name="${prefix}link_2_l">
            <visual>
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/visual/mh12_link_2_l.stl"/>
                </geometry>
                <xacro:material_yaskawa_blue/>
            </visual>
            <collision>
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/collision/mh12_link_2_l.stl"/>
                </geometry>
            </collision>
            <inertial>
                <xacro:cylinder_inertia m="5" r="0.4" h="1.1"/>
            </inertial>
        </link>
        <link name="${prefix}link_3_u">
            <visual>
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/visual/mh12_link_3_u.stl"/>
                </geometry>
                <xacro:material_yaskawa_blue/>
            </visual>
            <collision>
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/collision/mh12_link_3_u.stl"/>
                </geometry>
            </collision>
            <inertial>
                <xacro:cylinder_inertia m="5" r="0.6" h="0.5"/>
            </inertial>
        </link>
        <link name="${prefix}link_4_r">
            <visual>
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/visual/mh12_link_4_r.stl"/>
                </geometry>
                <xacro:material_yaskawa_blue/>
            </visual>
            <collision>
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/collision/mh12_link_4_r.stl"/>
                </geometry>
            </collision>
            <inertial>
                <xacro:cylinder_inertia m="3" r="0.4" h="0.4"/>
            </inertial>
        </link>
        <link name="${prefix}link_5_b">
            <visual>
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/visual/mh12_link_5_b.stl"/>
                </geometry>
                <xacro:material_yaskawa_blue/>
            </visual>
            <collision>
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/collision/mh12_link_5_b.stl"/>
                </geometry>
            </collision>
            <inertial>
                <xacro:cylinder_inertia m="2" r="0.3" h="0.2"/>
            </inertial>
        </link>
        <link name="${prefix}link_6_t">
            <visual>
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/visual/mh12_link_6_t.stl"/>
                </geometry>
                <xacro:material_yaskawa_blue/>
            </visual>
            <collision>
                <geometry>
                    <mesh filename="file:///$(find cell_envir)/urdf/motoman_mh12_support/meshes/collision/mh12_link_6_t.stl"/>
                </geometry>
            </collision>
            <inertial>
                <xacro:cylinder_inertia m="0.5" r="0.7" h="0.3"/>
            </inertial>
        </link>
        <link name="${prefix}tool0"/>
        <!-- end of link list -->

        <!-- joint list -->
        <joint name="${prefix}group_1/joint_1" type="revolute">
            <parent link="${prefix}base_link"/>
            <child link="${prefix}link_1_s"/>
            <origin xyz="0 0 0.450" rpy="0 0 0" />
            <axis xyz="0 0 1" />
        <limit  lower="-1.57" upper="1.57" effort="648.27" velocity="3.8397" />
        </joint>
        <joint name="${prefix}group_1/joint_2" type="revolute">
            <parent link="${prefix}link_1_s"/>
            <child link="${prefix}link_2_l"/>
            <origin xyz="0.155 0 0" rpy="0 0 0" />
            <axis xyz="0 1 0" />
            <limit lower="-1.57" upper="1.57" effort="874.65" velocity="3.4906"/>
        </joint>
        <joint name="${prefix}group_1/joint_3" type="revolute">
            <parent link="${prefix}link_2_l"/>
            <child link="${prefix}link_3_u"/>
            <origin xyz="0 0 0.614" rpy="0 0 0" />
            <axis xyz="0 -1 0" />
            <limit lower="-1.57" upper="1.57" effort="257.25" velocity="3.8397"/>
        </joint>
        <joint name="${prefix}group_1/joint_4" type="revolute">
            <parent link="${prefix}link_3_u"/>
            <child link="${prefix}link_4_r"/>
            <origin xyz="0.640 0 0.200" rpy="0 0 0" />
            <axis xyz="-1 0 0" />
            <limit lower="-1.57" upper="1.57" effort="74.97" velocity="7.1558"/>
        </joint>
        <joint name="${prefix}group_1/joint_5" type="revolute">
            <parent link="${prefix}link_4_r"/>
            <child link="${prefix}link_5_b"/>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <axis xyz="0 -1 0" />
            <limit lower="-2.57" upper="2.57" effort="44.69" velocity="7.1558"/>
        </joint>
        <joint name="${prefix}group_1/joint_6" type="revolute">
            <parent link="${prefix}link_5_b"/>
            <child link="${prefix}link_6_t"/>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <axis xyz="-1 0 0" />
            <limit lower="-2.57" upper="2.57" effort="14.80" velocity="10.6465"/>
        </joint>
        <joint name="${prefix}joint_6_t-tool0" type="fixed">
            <origin xyz="0.100 0 0" rpy="3.1415926 -1.570796 0"/>
            <parent link="${prefix}link_6_t"/>
            <child link="${prefix}tool0"/>
        </joint>
        <!-- end of joint list -->

        <!-- ROS base_link to Robot Manufacturer World Coordinates transform 
        <link name="${prefix}base" />
        <joint name="${prefix}base_link-base" type="fixed">
            <origin xyz="0 0 0.450" rpy="0 0 0"/>
            <parent link="${prefix}base_link"/>
            <child link="${prefix}base"/>
        </joint>
        -->

    </xacro:macro>
</robot>

